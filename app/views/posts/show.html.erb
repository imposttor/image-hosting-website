<p id="notice"><%= notice %></p>
<div>
    <%= image_tag(@post.image, width: 600) %>
</div>
<div>
    <%= render_with_hashtags(@post.hashtags) %>
</div>
<% if @post.user_id == current_user.id %>
<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
<%end%>
<%= react_component("share/Share") %>
